<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>



<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21">
   <children>
      <VBox prefHeight="500.0" prefWidth="700.0">
         <children>
            <Label fx:id="loginLabel" text="Login">
               <font>
                  <Font size="26.0" />
               </font>
               <VBox.margin>
                  <Insets left="300.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="userIDlabel" text="User ID:">
               <font>
                  <Font size="14.0" />
               </font>
               <VBox.margin>
                  <Insets left="20.0" top="20.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="userIDTextField" maxWidth="300.0">
               <VBox.margin>
                  <Insets left="100.0" top="-20.0" />
               </VBox.margin>
            </TextField>
            <Label fx:id="passwordLabel" text="Password:">
               <VBox.margin>
                  <Insets left="20.0" top="30.0" />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="passwordTextField" maxWidth="300.0">
               <VBox.margin>
                  <Insets left="100.0" top="-20.0" />
               </VBox.margin>
            </TextField>
            <Button fx:id="resetButton" mnemonicParsing="false" prefWidth="70.0" text="Reset">
               <VBox.margin>
                  <Insets left="600.0" top="40.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="loginButton" mnemonicParsing="false" prefWidth="120.0" text="Login">
               <VBox.margin>
                  <Insets left="20.0" top="-30.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" prefWidth="120.0" text="Forgot Password?">
               <VBox.margin>
                  <Insets left="320.0" top="-25.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
